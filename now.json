{
  "name": "music-cp-now1",
  "version": 2,
  "builds": [
    {
      "src": "server/server.ts",
      "use": "@now/node"
    },
    {
      "src": "server/list.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/api/video/info",
      "dest": "server/list.ts"
    },
    {
      "src": "/(.*)",
      "dest": "server/server.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
