    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js",
    "dev:server": "cross-env NODE_ENV=development nodemon --watch 'server/**/*.ts' --exec 'ts-node' server/server.ts",
    "server:server": "cross-env NODE_ENV=production nodemon --watch 'server/**/*.ts' --exec 'ts-node' server/server.ts",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "clear": "rimraf dist",
    "start": "node server/server.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint-staged": "lint-staged"
  },
